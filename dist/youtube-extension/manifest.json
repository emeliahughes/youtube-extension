{
  "manifest_version": 2,
  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Viblio"
  },
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/watch*"
      ],
      "css": [
        "content.css"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "name": "youtube-extension",
  "description": "Description",
  "version": "0.0.1"
}